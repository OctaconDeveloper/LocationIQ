{
  "name": "@octacondeveloper/locationiq",
  "version": "3.1.1",
  "description": "Pure, lightweight LocationIQ Places API client for Node.js and browsers",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "lint": "eslint src/**",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "@eslint/js": "^9.39.1",
    "globals": "^16.5.0"
  }
}